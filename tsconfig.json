{
    "compilerOptions": {
      "moduleResolution": "Node",
      "noEmitOnError": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "skipLibCheck": true,
      "outDir": "lib",
      "sourceMap": false,
      "strict": true,
      "target": "ES6"
    },
    "include": [
      "src/framework/**/*.ts",
      "src/server/**/*.ts"
    ]
}
