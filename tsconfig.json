{
    "compilerOptions": {
      "module": "CommonJS",
      "moduleResolution": "Node",
      "noEmitOnError": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "skipLibCheck": true,
      "outDir": "lib",
      "sourceMap": false,
      "strict": true,
      "target": "ES6"
    }
}
